APP_NAME=IntelicoleApi
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=https://api.intelicole.pe
FRONTEND_URL=https://intelicole.pe

APP_LOCALE=es
APP_FALLBACK_LOCALE=en

BCRYPT_ROUNDS=12

LOG_CHANNEL=stderr
LOG_LEVEL=warning

DB_CONNECTION=mysql
DB_SOCKET=/cloudsql/YOUR_PROJECT:YOUR_REGION:YOUR_INSTANCE
DB_DATABASE=your_database
DB_USERNAME=your_db_user
DB_PASSWORD=your_db_password

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=.intelicole.pe
SESSION_SECURE_COOKIE=true
SESSION_HTTP_ONLY=true
SESSION_SAME_SITE=none

CACHE_STORE=database

QUEUE_CONNECTION=database

SANCTUM_STATEFUL_DOMAINS=intelicole.pe,*.intelicole.pe,localhost:3000
CORS_ALLOWED_ORIGINS=https://intelicole.pe,http://localhost:3000

FILESYSTEM_DISK=gcs
GOOGLE_CLOUD_PROJECT_ID=peeposasistencia-482223
GOOGLE_CLOUD_STORAGE_BUCKET=peeposasistencia-storage

GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI=https://api.intelicole.pe/auth/google/callback

WAHA_API_BASE_IP=your_waha_ip
WAHA_TOKEN=your_waha_token

MAIL_MAILER=log
MAIL_FROM_ADDRESS="noreply@intelicole.pe"
MAIL_FROM_NAME="${APP_NAME}"

BIOMETRIC_ENABLED=false
FACE_SERVICE_URL=https://face-service-198597447144.us-central1.run.app
FACE_SERVICE_TIMEOUT=30

CARNET_CLEANUP_DAYS=7
CARNET_SERVICE_URL=https://carnet-service-XXXXXXXXXX-uc.a.run.app
CARNET_SERVICE_SECRET=your_64_char_hex_secret

SEED_SUPERADMIN_EMAIL=admin@intelicole.pe
